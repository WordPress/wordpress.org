{
	"name": "wporg-pattern-directory-theme",
	"version": "0.0.0",
	"description": "Theme for wordpress.org/patterns",
	"author": "WordPress.org",
	"license": "GPL-2.0-or-later",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/pattern-directory/"
	},
	"scripts": {
		"start": "grunt watch",
		"build": "NODE_ENV=production grunt build",
		"build:css": "grunt css",
		"build:js": "grunt js",
		"dev": "grunt",
		"lint:css": "wp-scripts lint-style 'css/**/*.scss'",
		"lint:js": "wp-scripts lint-js src",
		"format:js": "wp-scripts format src -- --config=../../../../.prettierrc.js",
		"test:unit": "wp-scripts test-unit-js",
		"packages-update": "wp-scripts packages-update"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"devDependencies": {
		"@lodder/grunt-postcss": "3.1.1",
		"@wordpress/a11y": "3.12.0",
		"@wordpress/api-fetch": "6.9.0",
		"@wordpress/block-editor": "9.4.0",
		"@wordpress/browserslist-config": "4.1.2",
		"@wordpress/components": "19.14.0",
		"@wordpress/compose": "5.10.0",
		"@wordpress/core-data": "4.10.0",
		"@wordpress/data": "6.12.0",
		"@wordpress/data-controls": "2.12.0",
		"@wordpress/dom": "3.12.0",
		"@wordpress/element": "4.10.0",
		"@wordpress/html-entities": "3.12.0",
		"@wordpress/i18n": "4.12.0",
		"@wordpress/icons": "9.3.0",
		"@wordpress/keycodes": "3.12.0",
		"@wordpress/primitives": "3.10.0",
		"@wordpress/scripts": "23.4.0",
		"@wordpress/url": "3.13.0",
		"@wordpress/viewport": "4.10.0",
		"autoprefixer": "10.4.7",
		"classnames": "2.3.1",
		"cssnano": "5.1.12",
		"grunt": "1.5.3",
		"grunt-contrib-clean": "2.0.1",
		"grunt-contrib-watch": "1.1.0",
		"grunt-sass": "3.1.0",
		"grunt-sass-globbing": "1.5.1",
		"grunt-webpack": "5.0.0",
		"lodash": "4.17.21",
		"postcss": "8.4.14",
		"react-use-gesture": "9.1.3",
		"rtlcss": "3.5.0",
		"sass": "1.53.0",
		"use-deep-compare-effect": "1.8.1"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js",
		"globals": {
			"wporgLocale": "readonly",
			"wporgPatternsData": "readonly",
			"wporgPatternsUrl": "readonly"
		}
	},
	"stylelint": {
		"extends": "../../../../.stylelintrc",
		"ignoreFiles": [
			"**/*.css",
			"**/*.css.map"
		]
	},
	"dependencies": {}
}
